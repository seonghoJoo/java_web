<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="likes">

	<select id="selectLikesList" parameterType="int" resultType="Like">
		SELECT no, regdate,liker_no likerNo, post_no postNo
		FROM posts
		WHERE post_no = #{no}
	</select>

</mapper>
